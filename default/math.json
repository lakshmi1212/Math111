{
  "agent_role": "ci_workflow_16generator",
  "repository": {
    "url": "https://github.com/lakshmi1212/Math111",
    "branch": "feature1"
  },
  "workflow": {
    "file_path": ".github/workflows/ci.yml",
    "job_id": "math_ci_job",
    "job_name": "Math CI Job"
  },
  "triggers": {
    "push": [".github/workflows"],
    "pull_request": ["feature1"]
  },
  "runtime": {
    "language": "python",
    "version": "3.10"
  },
  "dependencies": [
    "pytest==8.0.0"
  ],
  "test_execution": {
    "test_directory": "tests",
    "pytest_command": "python -m pytest tests/ -v --tb=short",
    "fail_on_error": true
  },
  "requirements_file": "default/requirements.txt",
  "constraints": {
    "modify_source_files": false,
    "modify_test_files": false
  },
  "commit": {
    "enabled": true,
    "message": "Add math operations, tests, requirements, and JSON metadata for CI integration"
  },
  "jobs": [
    {
      "job_id": "math_ci_job",
      "runs_on": "ubuntu-latest",
      "steps": [
        {
          "step_name": "Checkout code",
          "action_type": "uses",
          "action": "actions/checkout@v3"
        },
        {
          "step_name": "Set up Python",
          "action_type": "uses",
          "action": "actions/setup-python@v4",
          "with": {
            "python-version": "3.10"
          }
        },
        {
          "step_name": "Install dependencies",
          "action_type": "run",
          "command": "pip install -r default/requirements.txt"
        },
        {
          "step_name": "Run tests",
          "action_type": "run",
          "command": "python -m pytest tests/ -v --tb=short"
        }
      ]
    }
  ]
}
